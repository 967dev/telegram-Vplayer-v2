{
    "name": "967player",
    "version": "1.0.1",
    "description": "Premium Music & Radio Player",
    "main": "main.electron.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/967dev/telegram-Vplayer-v2.git"
    },
    "scripts": {
        "start": "electron .",
        "dev": "npx http-server ./ -p 8080",
        "package-win": "electron-builder build --win --publish never"
    },
    "author": "Sonic Glow Team",
    "license": "MIT",
    "dependencies": {
        "electron-updater": "^6.1.7"
    },
    "devDependencies": {
        "electron": "^28.0.0",
        "electron-builder": "^24.9.1"
    },
    "build": {
        "appId": "com.967player.app",
        "productName": "967player",
        "directories": {
            "output": "dist"
        },
        "win": {
            "target": "nsis"
        },
        "publish": {
            "provider": "github",
            "owner": "967dev",
            "repo": "telegram-Vplayer-v2"
        }
    }
}